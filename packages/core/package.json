{
  "name": "nuxtignore-dev",
  "version": "2.0.0",
  "description": "Nuxt module for support of .nuxtignore.dev file",
  "keywords": [
    "nuxt",
    "module",
    "nuxtignore"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/websavva/nuxtignore-dev.git"
  },
  "license": "MIT",
  "author": "websavva",
  "engines": {
    "node": "20.x",
    "pnpm": "9.x"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "build": "pnpm run prepare && nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && pnpm run prepare",
    "dev": "nuxi dev playground",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2"
  },
  "peerDependencies": {
    "ignore": "^5.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "eslint": "^8.57.0",
    "nuxt": "^3.13.2",
    "typescript": "^5.6.2",
    "mlly": "^1.7.2",
    "prettier": "^3.3.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-config-prettier": "^9.1.0",
    "vue-tsc": "^2.1.6"
  }
}
