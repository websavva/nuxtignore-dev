{
    "name": "nuxtignore-dev",
    "version": "1.2.2",
    "description": "Nuxt module for support of .nuxtignore.dev file",
    "keywords": [
      "nuxt",
      "module"
    ],
    "repository": {
      "type": "git",
      "url": "git+https://github.com/websavva/nuxtignore-dev.git"
    },
    "license": "MIT",
    "author": "websavva",
    "engines": {
      "node": "20.x",
      "pnpm": "8.x"
    },
    "type": "module",
    "exports": {
      ".": {
        "types": "./dist/types.d.ts",
        "import": "./dist/module.mjs",
        "require": "./dist/module.cjs"
      }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
      "dist"
    ],
    "scripts": {
      "prepare": "nuxt-module-build prepare",
      "build": "pnpm run prepare && nuxt-module-build build",
      "prepack": "pnpm run build",
      "dev:prepare": "nuxt-module-build build --stub && pnpm run prepare && nuxi prepare playground",
      "dev:build": "nuxi build playground",
      "dev": "nuxi dev playground",
      "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags",
      "lint": "eslint .",
      "lint:fix": "prettier --write . && eslint . --fix",
      "test": "vitest run",
      "test:watch": "vitest watch"
    },
    "dependencies": {
      "@nuxt/kit": "^3.10.3"
    },
    "devDependencies": {
      "@types/node": "^20.11.22",
      "@nuxt/devtools": "latest",
      "@nuxt/eslint-config": "^0.2.0",
      "@nuxt/module-builder": "^0.5.5",
      "@nuxt/schema": "^3.10.3",
      "@nuxt/test-utils": "^3.11.0",
      "changelogen": "^0.5.5",
      "eslint": "^8.57.0",
      "nuxt": "^3.10.3",
      "vitest": "^1.3.1",
      "@playwright/test": "^1.42.1",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "prettier": "^3.2.5"
    }
  }
  